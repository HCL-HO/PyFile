<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- outer circle -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#00b6b5" />
        </shape>
    </item>
    <!-- center circle -->
    <item
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp">
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="#00b6b5" />
            <gradient
                android:angle="270"
                android:endColor="#00b6b5"
                android:startColor="#00b6b5" />
        </shape>
    </item>
</layer-list>
