<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1.0px" android:color="@color/gv_line" />
            <gradient android:angle="270.0" android:endColor="#07a4a1" android:startColor="#07a4a1" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <gradient android:angle="270.0" android:endColor="#19c7def2" android:startColor="#19c7def2" />
            <stroke android:width="1.0px" android:color="@color/gv_line" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#19c7def2" />
            <stroke android:width="1.0px" android:color="@color/gv_line" />
        </shape>
    </item>
</selector>